{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "esNext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "allowJs": true
  }
}
